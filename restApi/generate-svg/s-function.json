{
  "name": "generate-svg",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: lambda-googlecharts",
  "customName": false,
  "customRole": false,
  "handler": "restApi/generate-svg/handler.handler",
  "timeout": 10,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": [
      ".git",
      "node_modules/full-icu/node_modules"
    ],
    "envVars": [],
    "optimize": true
  },
  "endpoints": [
    {
      "path": "generate-svg",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {
        "integration.request.querystring.chartOptions": "method.request.querystring.chartOptions"
      },
      "requestTemplates": "$${apiRequestTemplate}",
      "responses": {
        "500": {
          "selectionPattern": "^\\[ServerError\\].*",
          "statusCode": "500"
        },
        "400": {
          "selectionPattern": "^\\[BadRequest\\].*",
          "statusCode": "400"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {
            "method.response.header.Content-Type": "'image/svg+xml'"
          },
          "responseModels": {},
          "responseTemplates": {
            "image/svg+xml": "$input.path('$')"
          }
        }
      }
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}",
    "NODE_ICU_DATA": "node_modules/full-icu"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
